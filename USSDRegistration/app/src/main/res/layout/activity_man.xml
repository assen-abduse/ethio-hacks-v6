<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:background="@android:color/white">
    <ImageView
            android:layout_width="105dp"
            android:layout_height="67dp" app:srcCompat="@android:drawable/sym_action_call"
            android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/call"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView3"/>
    <Button
            android:text="0"
            android:layout_width="126dp"
            android:layout_height="70dp"
            android:id="@+id/zero"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintEnd_toStartOf="@+id/hashtag" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toEndOf="@+id/star"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginBottom="88dp" app:layout_constraintBottom_toBottomOf="parent"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="#"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/hashtag"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintStart_toEndOf="@+id/zero" app:layout_constraintHorizontal_bias="0.5"
            android:background="@android:drawable/screen_background_light_transparent"
            android:layout_marginBottom="88dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="*"
            android:layout_width="0dp"
            android:layout_height="69dp"
            android:id="@+id/star"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/zero"
            android:background="@android:drawable/screen_background_light_transparent"
            android:layout_marginBottom="88dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="1"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/one"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/two"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/four"
            android:layout_marginLeft="15dp" android:layout_marginStart="15dp"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="2"
            android:layout_height="70dp" android:id="@+id/two"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintStart_toEndOf="@+id/one"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/three"
            android:layout_width="0dp" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/five" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginRight="8dp" android:layout_marginEnd="8dp"
            android:visibility="visible" android:cursorVisible="false"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="3"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/three"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            android:layout_marginEnd="15dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="15dp"
            app:layout_constraintStart_toEndOf="@+id/two"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/six"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="4"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/four"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/five"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/seven"
            android:layout_marginLeft="15dp" android:layout_marginStart="15dp"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="5"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/five" app:layout_constraintEnd_toStartOf="@+id/six" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toEndOf="@+id/four"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.5"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/eight"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="6"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/six"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintStart_toEndOf="@+id/five"
            app:layout_constraintHorizontal_bias="0.5" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/nine"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="7"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/seven"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintEnd_toStartOf="@+id/eight" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/star" android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="8"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/eight"
            app:layout_constraintEnd_toStartOf="@+id/nine"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintStart_toEndOf="@+id/seven" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/zero"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <Button
            android:text="9"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:id="@+id/nine"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintStart_toEndOf="@+id/eight" app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/hashtag"
            android:background="@android:drawable/screen_background_light_transparent"/>
    <TextView
            android:layout_width="416dp"
            android:layout_height="90dp"
            android:layout_marginBottom="52dp"
            app:layout_constraintBottom_toTopOf="@+id/two"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.6" android:id="@+id/text"
            android:background="@drawable/bg"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.588"/>
    <ImageView
            android:layout_width="108dp"
            android:layout_height="63dp" app:srcCompat="@android:drawable/presence_video_online"
            android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/call" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="110dp"
            android:layout_height="59dp" app:srcCompat="@android:drawable/ic_search_category_default"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toEndOf="@+id/call" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>